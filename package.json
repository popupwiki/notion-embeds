{
  "name": "bun-nextjs-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "tsc -b && next build",
    "start": "next start",
    "types:check": "tsc",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "unused:check": "knip",
    "check-light": "bun types:check && bun lint:check && bun format:check",
    "check-all": "bun check-light && bun unused:check && bun test --coverage",
    "fix-all": "bun lint:fix && bun format:fix"
  },
  "dependencies": {
    "@vercel/functions": "^1.4.2",
    "next": "15.0.0",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020"
  },
  "devDependencies": {
    "@types/bun": "^1.1.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@woohm402/eslint-config-react": "^0.8.0",
    "autoprefixer": "^10.4.20",
    "eslint": "8.57.1",
    "knip": "^5.33.3",
    "postcss": "^8",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
